{% block body %}
  <h1>Supprimer un projet</h1>
  <form action="/project/delete/:titre" method="DELETE">
      <label for="titre">Titre du projet:</label><br>
      <input type="text" id="titre" name="titre" placeholder="Titre"><br>
      <button type="submit">Supprimer</button><br>
  </form>
{% endblock %}